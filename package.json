{
  "name": "generator-webpack-angularjs",
  "version": "0.0.4",
  "description": "Creating an AngularJS + UI-Router app with Webpack",
  "homepage": "https://github.com/xshyamx/generator-webpack-angularjs",
  "author": {
    "name": "shyam",
    "email": "xshyamx@gmail.com",
    "url": "http://github.com/xshyamx"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "angularjs",
    "angular1",
    "webpack",
    "ui-router",
    "bootstrap",
    "font-awesome"
  ],
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.1.0",
    "minimist": ">=1.2.3",
    "node-notifier": ">=8.0.1",
    "prettier": "^1.18.2",
    "yargs-parser": ">=13.1.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^4.0.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/xshyamx/generator-webpack-angularjs.git",
  "license": "Apache-2.0"
}
